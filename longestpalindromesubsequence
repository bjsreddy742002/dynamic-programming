
#include <bits/stdc++.h>
using namespace std;
#define ll  long long
#define M 1000000007;
int dp[1001][1001];
int main() 
{
    ll t;cin>>t;
    while(t--)
    {
        string s;cin>>s;
        int n=s.size();
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<n-i;j++)
            {
                if(s[j]==s[i+j])
                {
                    if(i==0)
                    {
                        dp[j][j+i]=1;
                    }
                    else if(i==1)
                    {
                        dp[j][j+i]=min((max(dp[j][j+i-1],dp[j+1][j+i])+1),i+1);
                    }
                    else{
                        dp[j][j+i]=min((max(dp[j][j+i-1],dp[j+1][j+i])+2),i+1);
                    }
                }
                else{
                    dp[j][j+i]=min((max(dp[j][j+i-1],dp[j+1][j+i])),i+1);
                }
            }
        }
        cout<<dp[0][n-1]<<endl;
    } 
    return 0;
}
