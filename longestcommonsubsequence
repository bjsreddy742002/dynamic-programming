#include <bits/stdc++.h>
using namespace std;
#define ll  long long
#define M 1000000007;
int dp[1001][1001];
int main() 
{
    ll t;cin>>t;
    while(t--)
    {
        string s1,s2;cin>>s1>>s2;
        int n=s1.size();
        int m=s2.size();
        for(int i=1;i<n+1;i++)
        {
            for(int j=1;j<m+1;j++)
            {
                if(s1[i-1]==s2[j-1])
                {
                    dp[i][j]=dp[i-1][j-1]+1;
                }
                else{
                    dp[i][j]=(max(dp[i][j-1],dp[i-1][j]));
                }
            }
        }
        cout<<dp[n][m]<<endl;
    } 
    return 0;
}
